{
  "name": "svg-utils",
  "version": "1.1.0",
  "description": "Miscellaneous utility components for manipulating SVG",
  "main": "dist/svg-utils.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "build": "webpack",
    "watch": "webpack --progress --colors --watch",
    "minify": "uglifyjs dist/svg-utils.js -o dist/svg-utils.min.js -c -m --preamble \"$(preamble)\"",
    "prepublish": "npm run build && npm run minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vasturiano/svg-utils.git"
  },
  "keywords": [
    "svg",
    "component",
    "utils",
    "d3"
  ],
  "author": {
    "name": "Vasco Asturiano",
    "url": "http://bl.ocks.org/vasturiano"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vasturiano/svg-utils/issues"
  },
  "homepage": "https://github.com/vasturiano/svg-utils#readme",
  "dependencies": {
    "d3": "^4.10",
    "kapsule": "^1.5.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "uglify-js": "^3.1.0",
    "webpack": "^3.5.6"
  }
}
